{
  "name": "cetus-tools-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for Cetus Protocol tools - CLI and Web interface",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:all": "pnpm -r run build",
    "dev:cli": "pnpm --filter cli run dev",
    "dev:web": "pnpm --filter web run dev",
    "build:cli": "pnpm --filter cli run build",
    "build:web": "pnpm --filter web run build",
    "install:all": "pnpm install"
  },
  "keywords": [
    "cetus",
    "sui",
    "cli",
    "web",
    "defi",
    "monorepo"
  ],
  "author": "v1xingyue<qixingyue@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
